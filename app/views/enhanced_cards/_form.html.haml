= form_for [@card_level, @enhanced_card] do |f|
  - if @enhanced_card.errors.any?
    #error_explanation
      %h2= "#{pluralize(@enhanced_card.errors.count, "error")} prohibited this enhanced_card from being saved:"
      %ul
        - @enhanced_card.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :target_skill_level
    = f.number_field :target_skill_level, :in=>@eligible_card_levels
  .actions
    = f.submit 'Save'
